<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <% skillNames.forEach(function(skill) { %>
        <% if (id == skill.id) { %>
            <h1> <%= skill.name %> is at this URL with the id of <%= skill.id %></h1>
        <% } %>
    <% }) %>

    <a href="/skills">Back to skills list</a>
</body>
</html>